	// display popup on home page when visitor come to a site
	$(document).ready(function(){
		var seconds,cancel;
		var isshow = localStorage.getItem('isshow');
		var isshow_newsletterPopup = localStorage.getItem('isshow_newsletterPopup');
		
		if (sessionStorage.getItem("on_load_counter") === null || sessionStorage.getItem("on_load_counter") == 'NaN') {
			seconds = 0;
			sessionStorage.setItem("on_load_counter", seconds);
		} else {
			seconds = sessionStorage.getItem('on_load_counter');
		}
		
		function incrementSeconds() {
			seconds = parseInt(seconds) + 1;
			sessionStorage.setItem("on_load_counter", seconds);
			if(seconds == 30 && isshow === null) {				
				$('#myModal').modal();
			}
			if(seconds == 60 && isshow_newsletterPopup === null) {
				if($('#myModal').is(':visible')){
					$('#myModal').modal('hide');
				}
				$('#newsletterModal').modal();	
			}
		}
		cancel = setInterval(incrementSeconds, 1000);

		$('#chk_dnt_show').change(function() {
			if(this.checked == true){
				if (isshow === null) {
					localStorage.setItem('isshow', 1);			
				} 
			}
		});
		
		$('#chk_ndntshow').change(function() {
			if(this.checked == true){
				if (isshow_newsletterPopup === null) {					
					localStorage.setItem('isshow_newsletterPopup', 1);			
				} 
			}
		});
		
		$('#browse_website').click(function(){
			jQuery('#myModal').modal('hide');
		});
		$('.btn_first_pop').click(function(){
			jQuery('#newsletterModal').modal('hide');
		});
		$(".menu_nav").click(function(){
			$(".menu_nav, .menu_edited, .menu_nav_data").toggleClass("c_active");
